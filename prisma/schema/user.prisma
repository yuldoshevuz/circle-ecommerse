model User {
  id         String    @id @default(uuid())
  full_name  String
  email      String    @unique
  google_id  String?
  role       RoleUser  @default(USER)
  cart       Cart?
  orders     Order[]
  favourites Product[] @relation("UserFavourite")

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

enum RoleUser {
  USER
  ADMIN
}
